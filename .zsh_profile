export PATH="/usr/local/sbin:$PATH"


[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

export PATH="$PATH:$HOME/.rvm/bin"

# # postgresql path option
export LDFLAGS="-L/usr/local/opt/postgresql@10/lib"
export CPPFLAGS="-I/usr/local/opt/postgresql@10/include"
export PKG_CONFIG_PATH="/usr/local/opt/postgresql@10/lib/pkgconfig"
export PATH="$PATH:/usr/local/opt/postgresql@10/bin"

# # golang path option
export GOPATH="/Users/admin/Documents/goProject"
export PATH="$PATH:$GOPATH/bin"


# # openssl path option
export PATH="/usr/local/opt/openssl@1.1/bin:$PATH"
export LDFLAGS="-L/usr/local/opt/openssl@1.1/lib"
export CPPFLAGS="-I/usr/local/opt/openssl@1.1/include"
export PKG_CONFIG_PATH="/usr/local/opt/openssl@1.1/lib/pkgconfig:$PKG_CONFIG_PATH"

# # gnu utils
export PATH="/usr/local/opt/grep/libexec/gnubin:$PATH"
export PATH="/usr/local/opt/findutils/libexec/gnubin:$PATH"
export PATH="/usr/local/opt/gnu-which/libexec/gnubin:$PATH" 
export MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"

# # rust path option
export PATH="$PATH:$HOME/.cargo/bin"

# # MacPorts Installer addition on 2019-12-28_at_21:31:57: adding an appropriate PATH variable for use with MacPorts.
export PATH="/opt/local/bin:/opt/local/sbin:$PATH"

# Finished adapting your PATH environment variable for use with MacPorts.
complete -C /usr/local/bin/mc mc

